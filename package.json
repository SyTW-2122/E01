{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "test:prod": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "test:cov": "ng test --code-coverage --no-watch --no-progress --browsers=Firefox",
    "coverage": "nyc npm run test:cov && cat ./coverage/frontend/lcov.info  | coveralls",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "webdriver": "webdriver-manager start --standalone",
    "webdriver:update": "webdriver-manager update",
    "compile-protractor": "tsc -p tsconfig.e2e.json",
    "protractor": "protractor protractor.conf.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~13.3.0",
    "@angular/common": "~13.3.0",
    "@angular/compiler": "~13.3.0",
    "@angular/core": "~13.3.0",
    "@angular/forms": "~13.3.0",
    "@angular/platform-browser": "~13.3.0",
    "@angular/platform-browser-dynamic": "~13.3.0",
    "@angular/router": "~13.3.0",
    "backend": "file:server",
    "bootswatch": "^5.1.3",
    "puppeteer": "^13.6.0",
    "rxjs": "~7.5.0",
    "travis": "^0.1.1",
    "tslib": "^2.3.0",
    "webdriver-manager": "^12.1.8",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.3",
    "@angular/cli": "~13.3.3",
    "@angular/compiler-cli": "~13.3.0",
    "@angular/language-service": "^13.3.4",
    "@cypress/schematic": "^1.6.0",
    "@types/jasmine": "~3.10.0",
    "@types/jasminewd2": "^2.0.10",
    "@types/node": "^12.11.1",
    "codecov": "^3.8.3",
    "codelyzer": "^6.0.2",
    "coveralls": "^3.1.1",
    "cypress": "latest",
    "jasmine-core": "~4.0.0",
    "jasmine-spec-reporter": "^7.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-firefox-launcher": "^2.1.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "nyc": "^15.1.0",
    "protractor": "^7.0.0",
    "ts-node": "^10.7.0",
    "tslint": "^6.1.3",
    "typescript": "~4.6.2"
  }
}
