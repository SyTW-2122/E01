{
  "exclude": [
    "./server",
    "./cypress",
    "./src"
  ],
  "compilerOptions": {
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "node",
    "importHelpers": true,
    "outDir": "./out-tsc/e2e",
    "baseUrl": "./",
    "module": "CommonJS",
    "target": "ES6",
    "types": [
      "jasmine",
      "jasminewd2",
      "node"
    ]
  }
}
